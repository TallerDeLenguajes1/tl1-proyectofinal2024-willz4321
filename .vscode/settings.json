{
    "dotnet.preferCSharpExtension": true,
   "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net8.0/Program.dll",
            "args": [],
            "cwd": "${workspaceFolder}/bin/Debug/net8.0",
            "stopAtEntry": false,
            "console": "internalConsole",
            "logging": {
                "moduleLoad": false
            },
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}